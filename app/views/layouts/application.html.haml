!!! 5
%html
%head
  %title Rumble12

  = stylesheet_link_tag    "application", :media => "all"
  = javascript_include_tag "application"
  = csrf_meta_tags

%body

  .navbar.navbar-fixed-top
    .navbar-inner
      .container
        = link_to "SpongeHub.io", root_path, class: "brand"
        %ul.nav
          %li.active
            =link_to "Home", root_path

        %ul.nav.pull-right
          %li.dropdown
            =link_to "#", class: "dropdown-toggle", data: {toggle: "dropdown"} do
              Spaces
              %b.caret
            %ul.dropdown-menu
              %li= link_to "Space 1", "#"
              %li= link_to "Space 2", "#"
              %li= link_to "Space 3", "#"
          %li
            =link_to "Sign Up"
          %li
            =link_to "Login"

  .container
    = yield
